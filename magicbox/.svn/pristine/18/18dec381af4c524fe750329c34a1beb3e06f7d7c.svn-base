@CHARSET "UTF-8";

html{
	padding: 0;
}
a {
	text-decoration: none;
	color: #444;
	border-bottom: 1px solid #ccc;
}
a:hover {
	border-bottom: 1px solid #444;
}
.tipToDev {
	padding: 0.9em 20px 1em 20px;
	font-size: 95%;
	line-height: 1.2em;
	border-bottom: 1px solid #eaeaea;
	background: #fefeee;
	color: #00ac73;
}
.chrome {
	position: absolute;
	left: 162px;
	top: 0;
	right: 0;
	bottom: 165px;
	background: #fff;
}
.myiframe {
	position: static;
	width: 100%;
	padding: 0;
	margin: 0;
	background: #fff;
}
#inner_body {
	overflow: auto;
	padding: 0;
	height: 100%;
	color: #444;
	text-shadow: 0 1px 0 #fff;
	font-size: 12px;
	line-height: 1.5em;
	font-family: "Microsoft YaHei", "Helvetica Neue", Helvetica, Arial, sans-serif;
}
.text {
	font-family: "Hiragino Sans GB", "Microsoft YaHei", sans-serif;
	font-size: 12px;
	color: #444;
}

h1, h2, h3, h4, h5, h6 {
	font-weight: normal;
	line-height: 1.8em;
}
h2 {
	font-size: 22px;
	padding-bottom: 0.5em;
}
.label {
	display: block;
	float: left;
	height: 32px;
	line-height: 34px;
	vertical-align: middle;
}
.normal_txt {
	height: 14px;
	line-height: 14px;
	padding: 7px 8px;
	vertical-align: middle;
	border: 1px solid #d5d5d5;
	border-radius: 2px;
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	-o-border-radius: 2px;
	box-shadow: inset 0 1px 3px #d5d5d5;
	background: #f8f8f8;
}
.normal_txt:focus {outline: none; background: #fff;}
.textarea {
	height: 80px;
	min-height: 80px;
	max-height: 200px;
	max-width: 420px;
	min-width: 200px;
	padding: 5px 10px;
	overflow: visible;
	overflow-y: auto;
	line-height: 1.4em;
}
select {
	-webkit-appearance: button;
	-webkit-border-radius: 2px;
	-webkit-box-shadow: 0 1px 3px rgba(0,0,0,.1);
	-webkit-padding-end: 20px;
	-webkit-padding-start: 2px;
	-webkit-user-select: none;
	
	-moz-appearance: button;
	-moz-border-radius: 2px;
	-moz-box-shadow: 0 1px 3px rgba(0,0,0,.1);
	-moz-padding-end: 20px;
	-moz-padding-start: 2px;
	-moz-user-select: none;
	
	background-image: url(../images/arrow_down.png), -webkit-linear-gradient(top, #f5f5f5 40%, #f1f1f1);
	background-image: url(../images/arrow_down.png), -moz-linear-gradient(top, #f5f5f5 40%, #f1f1f1);
	background-position: center right;
	background-repeat: no-repeat;
	
	min-width: 165px;
	margin: 0;
	padding: 5px 0;
	border: 1px solid #d5d5d5;
	font-size: inherit;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.normal_btn {
    padding: 5px 10px;
    border: 1px solid #eaeaea;
    color: #444;
	border-radius: 2px;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    -o-border-radius: 2px;
    cursor: pointer;
    vertical-align: middle;
    text-align: center;
    background-image: -webkit-linear-gradient(top, #f5f5f5, #f1f1f1);
	background-image: -moz-linear-gradient(top, #f5f5f5, #f1f1f1);
	background-image: -ms-linear-gradient(top, #f5f5f5, #f1f1f1);
    background-image: -o-linear-gradient(top, #f5f5f5, #f1f1f1);
    background-color: #f5f5f5;
}
.normal_btn:hover {
    border: 1px solid #dcdcdc;
    -webkit-transition: background 0.25s linear;
    background-image: -webkit-linear-gradient(top, #f8f8f8, #f1f1f1);
	background-image: -moz-linear-gradient(top, #f8f8f8, #f1f1f1);
	background-image: -ms-linear-gradient(top, #f8f8f8, #f1f1f1);
    background-image: -o-linear-gradient(top, #f8f8f8, #f1f1f1);
    background-color: #f8f8f8;
}
.normal_btn:active {
	background-position: 0% 0%;
    box-shadow: inset 0 1px 10px #979797;
    background-image: -webkit-linear-gradient(top, #f8f8f8, #f1f1f1);
	background-image: -moz-linear-gradient(top, #f8f8f8, #f1f1f1);
	background-image: -ms-linear-gradient(top, #f8f8f8, #f1f1f1);
    background-image: -o-linear-gradient(top, #f8f8f8, #f1f1f1);
    background-color: #f8f8f8;
    background-repeat: repeat;
    background-attachment: scroll;
}
.btn_b {
	background-position: 0% 0%;
    min-width: 65px;
	border: 1px solid #0062d9;
    color: #f5f5f5;
    background-image: -webkit-linear-gradient(top, #0077d3, #004ba6);
	background-image: -moz-linear-gradient(top, #0077d3, #004ba6);
	background-image: -ms-linear-gradient(top, #0077d3, #004ba6);
    background-image: -o-linear-gradient(top, #0077d3, #004ba6);
    background-color: #0055c0;
    background-repeat: repeat;
    background-attachment: scroll;
    text-shadow: 0 1px 0 #444;
}
.btn_b:hover {
	background: #0055c0;
	background-image: -webkit-linear-gradient(top, #0055c0, #004ba6);
	background-image: -moz-linear-gradient(top, #0055c0, #004ba6);
	background-image: -ms-linear-gradient(top, #0055c0, #004ba6);
	background-image: -o-linear-gradient(top, #0055c0, #004ba6);
	border: 1px solid #0062d9;
	color: #fff;
}
.btn_b:active {
    box-shadow: inset 0 1px 10px #003473;
    background-image: -webkit-linear-gradient(bottom, #0055c0, #004ba6);
	background-image: -moz-linear-gradient(bottom, #0055c0, #004ba6);
	background-image: -ms-linear-gradient(bottom, #0055c0, #004ba6);
    background-image: -o-linear-gradient(bottom, #0055c0, #004ba6);
    background-color: #0055c0;
}
::-webkit-scrollbar {
	width: 10px;
	height: 10px;
}
::-webkit-scrollbar-track {
	-webkit-box-shadow: inset 0 0 5px rgba(0,0,0,0.3);
	-webkit-border-radius: 10px;
	border-radius: 10px;
}
::-webkit-scrollbar-thumb {
	-webkit-border-radius: 10px;
	border-radius: 10px;
	background: #eaeaea;
	-webkit-box-shadow: inset 0 0 5px rgba(0,0,0,0.5);
}
::-webkit-scrollbar-thumb:window-inactive {
	background: #eaeaea;
}
::-moz-scrollbar {
	width: 10px;
	height: 10px;
}
.textellipsis {
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}

