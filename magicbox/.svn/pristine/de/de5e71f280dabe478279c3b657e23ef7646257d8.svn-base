div.table table.uploadfade tr {
    border-top: 1px solid #ffffff;
    border-bottom: 1px solid #e5e5e5;
}
div.table table.uploadfade td{
    border: none;
}
div.table table.uploadfade tr.succeed td {
    background: #f1f1f8;
}
table.uploadfade .uploadfade-item .upload-checkbox {
    width: 18px;
}
table.uploadfade .uploadfade-item .image-preview {
    width: 52px;
}
table.uploadfade .uploadfade-item .image-preview img {
    max-width: 52px;
}
table.uploadfade .uploadfade-item .image-title { width: 92px; }
table.uploadfade .uploadfade-item .image-title div {
    width: 92px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
table.uploadfade .uploadfade-item .upload-progress { width: 252px;}
